    <style>
        /* Portfolio-specific styles */
        .project-content {
            max-width: 1000px;
            margin: 0 auto;
            padding: 2rem;
            background-color: var(--bg-lighter);
            border-radius: var(--card-border-radius);
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
            border: 1px solid var(--bg-highlight);
        }
        
        .project-header {
            text-align: center;
            margin-bottom: 2rem;
        }
        
        .project-header h1 {
            color: var(--aqua);
            margin-bottom: 1rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        .project-meta {
            margin-bottom: 2rem;
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            gap: 1.5rem;
        }
        
        .project-meta-item {
            display: flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.5rem 1rem;
            background-color: var(--bg-light);
            border-radius: 8px;
            border: 1px solid var(--bg-accent);
        }
        
        .project-meta-item i {
            color: var(--aqua);
        }
        
        .project-gallery {
            margin: 2rem 0;
            display: grid;
            grid-template-columns: repeat(auto-fill, minmax(300px, 1fr));
            gap: 1.5rem;
        }
        
        .gallery-item {
            border-radius: var(--card-border-radius);
            overflow: hidden;
            box-shadow: 0 3px 10px rgba(0, 0, 0, 0.3);
            border: 1px solid var(--bg-accent);
        }
        
        .gallery-item img {
            width: 100%;
            height: auto;
            display: block;
            transition: transform 0.3s;
        }
        
        .gallery-item:hover img {
            transform: scale(1.05);
        }
        
        .project-description {
            margin-bottom: 2rem;
        }
        
        .project-description h2 {
            color: var(--green);
            margin-bottom: 1rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        .project-description p {
            margin-bottom: 1.5rem;
        }
        
        .project-features {
            margin-bottom: 2rem;
            padding: 1.5rem;
            background-color: var(--bg-light);
            border-radius: var(--card-border-radius);
        }
        
        .project-features h2 {
            color: var(--green);
            margin-bottom: 1.5rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        .project-features ul {
            list-style-type: none;
            padding-left: 0;
        }
        
        .project-features li {
            margin-bottom: 1rem;
            padding-left: 1.8rem;
            position: relative;
        }
        
        .project-features li::before {
            content: "\f00c";
            font-family: "Font Awesome 5 Free";
            font-weight: 900;
            position: absolute;
            left: 0;
            color: var(--aqua);
        }
        
        .project-tech {
            margin-bottom: 2rem;
        }
        
        .project-tech h2 {
            color: var(--blue);
            margin-bottom: 1.5rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        .tech-stack {
            display: flex;
            flex-wrap: wrap;
            gap: 0.8rem;
            margin-top: 1rem;
        }
        
        .tech-tag {
            background-color: var(--bg-highlight);
            color: var(--fg-light);
            padding: 0.4rem 0.8rem;
            border-radius: 20px;
            font-size: 0.9rem;
            font-weight: 500;
            border: 1px solid var(--bg-accent);
            transition: background-color 0.3s, transform 0.2s;
        }
        
        .tech-tag:hover {
            background-color: var(--bg-accent);
            transform: translateY(-2px);
            border-color: var(--aqua);
        }
        
        .project-links {
            display: flex;
            gap: 1rem;
            margin-top: 2rem;
            flex-wrap: wrap;
        }
        
        .project-link-btn {
            display: inline-flex;
            align-items: center;
            gap: 0.5rem;
            padding: 0.8rem 1.5rem;
            background-color: var(--aqua);
            color: var(--bg-darker);
            text-decoration: none;
            border-radius: 8px;
            transition: background-color 0.3s, transform 0.2s;
            font-weight: bold;
        }
        
        .project-link-btn:hover {
            background-color: var(--blue);
            transform: translateY(-3px);
        }
        
        .project-challenges {
            margin-bottom: 2rem;
            padding: 1.5rem;
            background-color: var(--bg-light);
            border-radius: var(--card-border-radius);
        }
        
        .project-challenges h2 {
            color: var(--blue);
            margin-bottom: 1.5rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        .project-learning {
            margin-bottom: 2rem;
            padding: 1.5rem;
            background-color: var(--bg-light);
            border-radius: var(--card-border-radius);
        }
        
        .project-learning h2 {
            color: var(--green);
            margin-bottom: 1.5rem;
            font-family: 'FantasqueSansMonoBold', 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        }
        
        @media (max-width: 768px) {
            .project-content {
                padding: 1.5rem;
            }
            
            .project-gallery {
                grid-template-columns: 1fr;
            }
            
            .project-links {
                flex-direction: column;
            }
        }
    </style>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
